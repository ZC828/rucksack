      <guid>https://simplaio.github.io/rucksack/home/autoprefixing/</guid>
      <description>&lt;pre&gt;&lt;code class=&#34;language-css&#34;&gt;.foo {
  transition: all;
}

/* Becomes (from CanIUse) */
